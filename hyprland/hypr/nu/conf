env = HYPRLAND_STATE,~/.cache/nushell/hyprland.db
exec-once = nu ~/.config/hypr/nu/init.nu 
exec-once = nu ~/.config/hypr/nu/listen.nu
bind = ALT,Tab, exec, nu ~/.config/hypr/nu/cycle-workspace.nu cycle
bindr = ALT,Tab, exec, nu ~/.config/hypr/nu/cycle-workspace.nu pop

$editor = nu -c 'source ~/.env.nu; source ~/Configuration/nushell/scripts/main/env.nu; source ~/.nu; cd ~; neovide --frame=none --maximized --vsync'
